<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.view.myPage.MyPageFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/iv_profile_img"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/bg_timeline_profile_img"
            android:src="@drawable/sample_profile_img1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_close_btn"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="28dp"
            android:backgroundTint="@color/Grey_600"
            android:src="@drawable/ic_x"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_profile_name"
            style="@style/Headline7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="10dp"
            android:text="바키"
            android:textColor="@color/Grey_600"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile_img" />

        <TextView
            android:id="@+id/tv_username"
            style="@style/Body1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:text="\@_parking1_"
            android:textColor="@color/BlueGrey_800"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_profile_name" />

        <TextView
            android:id="@+id/tv_following_num"
            style="@style/Headline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="12dp"
            android:text="4"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_username" />

        <TextView
            android:id="@+id/tv_following"
            style="@style/Body2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginTop="12dp"
            android:text="팔로잉"
            android:textColor="@color/BlueGrey_800"
            app:layout_constraintStart_toEndOf="@+id/tv_following_num"
            app:layout_constraintTop_toBottomOf="@+id/tv_username" />

        <TextView
            android:id="@+id/tv_follower_num"
            style="@style/Headline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="12dp"
            android:text="4"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/tv_following"
            app:layout_constraintTop_toBottomOf="@+id/tv_username" />

        <TextView
            android:id="@+id/tv_follower"
            style="@style/Body2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginTop="12dp"
            android:text="팔로워"
            android:textColor="@color/BlueGrey_800"
            app:layout_constraintStart_toEndOf="@+id/tv_follower_num"
            app:layout_constraintTop_toBottomOf="@+id/tv_username" />

        <ImageView
            android:id="@+id/iv_profile_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="34dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_user"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_following" />

        <TextView
            android:id="@+id/tv_profile_menu"
            style="@style/Headline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:text="프로필"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/iv_profile_icon"
            app:layout_constraintTop_toBottomOf="@+id/tv_following" />

        <ImageView
            android:id="@+id/iv_bookmark_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/ic_bookmark"
            android:backgroundTint="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile_icon" />

        <TextView
            android:id="@+id/tv_bookmark_menu"
            style="@style/Headline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:text="북마크"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/iv_bookmark_icon"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile_icon" />

        <ImageView
            android:id="@+id/iv_list_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_list"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_bookmark_icon" />

        <TextView
            android:id="@+id/tv_list_menu"
            style="@style/Headline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:text="리스트"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/iv_list_icon"
            app:layout_constraintTop_toBottomOf="@+id/iv_bookmark_icon" />

        <ImageView
            android:id="@+id/iv_space_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_mic"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_list_icon" />

        <TextView
            android:id="@+id/tv_space_menu"
            style="@style/Headline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:text="스페이스"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/iv_space_icon"
            app:layout_constraintTop_toBottomOf="@+id/iv_list_icon" />

        <ImageView
            android:id="@+id/iv_money_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_money"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_space_icon" />

        <TextView
            android:id="@+id/tv_money_menu"
            style="@style/Headline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:text="수익창출"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/iv_money_icon"
            app:layout_constraintTop_toBottomOf="@+id/iv_space_icon" />

        <View
            android:id="@+id/v_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="28dp"
            android:background="@color/BlueGrey_200"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_money_icon" />

        <TextView
            android:id="@+id/tv_setting"
            style="@style/Headline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="40dp"
            android:text="설정 및 지원"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/v_line" />

        <ImageView
            android:id="@+id/ic_toggle_down"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="40dp"
            android:backgroundTint="@color/Grey_600"
            android:src="@drawable/ic_down_arrow"
            app:layout_constraintEnd_toEndOf="@+id/v_line"
            app:layout_constraintTop_toBottomOf="@+id/v_line" />

        <ImageView
            android:id="@+id/ic_setting_menu"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_setting"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_setting" />

        <TextView
            android:id="@+id/tv_setting_menu"
            style="@style/Body2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="22dp"
            android:layout_marginTop="25dp"
            android:text="설정 및 개인정보 보호"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/ic_setting_menu"
            app:layout_constraintTop_toBottomOf="@+id/tv_setting" />

        <ImageView
            android:id="@+id/ic_customer_menu"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_circle_question"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ic_setting_menu" />

        <TextView
            android:id="@+id/tv_customer_menu"
            style="@style/Body2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="22dp"
            android:layout_marginTop="25dp"
            android:text="고객센터"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/ic_setting_menu"
            app:layout_constraintTop_toBottomOf="@+id/ic_setting_menu" />

        <ImageView
            android:id="@+id/ic_perchase_menu"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/black"
            android:src="@drawable/ic_cart"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ic_customer_menu" />

        <TextView
            android:id="@+id/tv_perchase_menu"
            style="@style/Body2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="22dp"
            android:layout_marginTop="25dp"
            android:text="구매"
            android:textColor="@color/Grey_600"
            app:layout_constraintStart_toEndOf="@+id/ic_setting_menu"
            app:layout_constraintTop_toBottomOf="@+id/ic_customer_menu" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>
